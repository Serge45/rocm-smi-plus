{
  "name": "rocm-smi-plus",
  "displayName": "rocm-smi+",
  "description": "Show AMD GPU info in VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rocm-smi-plus.helloWorld",
        "title": "rocm-smi+: Hello World"
      },
      {
        "command": "rocm-smi-plus.refresh",
        "title": "Refresh",
        "icon": {
          "dark": "resources/dark/refresh.svg",
          "light": "resources/light/refresh.svg"
        }
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "rocm-smi-plus",
          "name": "rocm-smi+"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "rocm-smi-plus.refresh"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}
